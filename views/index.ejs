<!DOCTYPE html>
<html>
  <head>
  
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="/css/style.css" rel="stylesheet" type="text/css">
    

    <title><%= title %></title>
  
  </head>
  <body>
      <%- include('./partials/header') %>

<!-- new html below -->

<a class="mainpagea" href="/new">new Brew</a>

<% brews.forEach(function(b) { %>

    <p>Name: <%= b.name %></p>
    <p>Brewery: <%= b.brewery %></p>
    
    <a class="mainpagea" href="/<%=b._id%>">details</a>
    
    <form action="/<%= b._id %>?_method=DELETE" method="POST" class="secondary-content">
      <button type="submit" class="btn-small red-text">X</button>
    </form>





  <% }); %>
  
<p></p>



  </body>
</html>
